
page{
    background: #f1f2f7;
    border-top: 1px solid #E4E7F0;
}

.container{
    display: flex;flex-direction: column;
    width:100%;
    height: 100%;
    margin:0;padding: 0;
}

.container .wrapper{
    display: flex;
    flex-direction: column;
}

.container .wrapper .ul{
    display: flex;
    flex-direction: column;
    padding: 0 40rpx;
}
.container .wrapper .ul .title{
    display: flex;
    justify-content: space-between;
    width: 100%;
    padding: 18rpx 40rpx;
    font-size: 28rpx;
    color: #fff;
    background: #BEC1E5;
    border-radius: 12rpx 12rpx 0 0;
    margin-top: 40rpx;
}
.container .wrapper .ul .title .types{
    border: 1px solid #5555ff;
    color: #fff;
    background: #5555ff;
}
.container .wrapper .ul .li{
    flex: 1;
    display: flex;
    padding: 30rpx 32rpx;
    border-radius: 0 0 12rpx 12rpx;
    background: #fff;
}
.container .wrapper .ul .li:first-child{  
    border-top: none; 
}
.container .wrapper .ul .l-box{
    display: flex;
    flex: 0 0 300rpx;
    height: 240rpx;
}
.container .wrapper .ul .l-box image{
    display: flex;
    width: 100%;
    height: 100%;
    border-radius: 8rpx;
}

.container .wrapper .ul .r-box{
    display: flex;
    flex: 1;
    flex-direction: column;
    padding-left: 28rpx;
    font-size: 32rpx;
}
.container .wrapper .ul .r-box .tit{
    font-size: 32rpx;color: #3B3A4A;
    height:44rpx;
    padding-top: 6rpx;
    margin-bottom: 8rpx;
    display: flex;align-items: center;  
}
.container .wrapper .ul .r-box .tit text:last-child{
    width: 226rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.container .wrapper .ul .r-box .tit text:first-child{
    width:64rpx;
    height:34rpx; 
    border-radius: 8rpx; 
    border: 2rpx solid #3B55E7;
    font-size:24rpx;color: #3B55E7;
    display: flex;
    justify-content: center;align-items: center;
    margin-right: 8rpx;
    box-sizing: border-box;
}
.container .wrapper .ul .r-box .addr{
    font-size: 24rpx;color:#828393;
    margin-bottom: 28rpx;
    overflow:hidden; 
    text-overflow:ellipsis;
    display:-webkit-box; 
    -webkit-box-orient:vertical;
    -webkit-line-clamp:1;
}
.container .wrapper .ul .r-box .biaoqian{
    display: flex;
}
.container .wrapper .ul .r-box .biaoqian text{
    display: flex;
    justify-content: center;align-items: center;
    padding: 0 8rpx;
    font-size: 24rpx;
    color: #fff;
    border-radius: 8rpx;
    margin-right: 8rpx;
    background: #BBBCCD;
}
.container .wrapper .ul .r-box .price{
    font-size: 28rpx;color: #F25C62;
    margin-bottom: 32rpx;
}

.noList,.noMore{
    display: flex;
    width: 100%;
    justify-content: center;
    font-size: 28rpx;
    padding: 20rpx 0;
}


.icon-sousuo{
    margin-top: -14rpx;
    margin-right: 6rpx;
}