page{
    background: #fff;
    border-top: 1px solid #E4E7F0;
}
.container{
    display: flex; 
    flex-direction: column;
    width: 100%;height: 100%;
}

.room-details{
    font-size: 28rpx;
    padding: 0 40rpx;
    display: flex;
    flex-direction: column;
    width: 100%;
    margin-top: 40rpx;
}
.room-details view{
    display: flex;
    width: 100%;
    margin-bottom: 16rpx;
    color: #828393;
    font-size: 28rpx;
    padding-bottom: 16rpx;
    border-bottom:1px dashed #E4E7F0;
}
.room-details view text:first-child{
    flex:0 0 176rpx;
    color: #BBBCCD;
    font-weight: 400;
}
