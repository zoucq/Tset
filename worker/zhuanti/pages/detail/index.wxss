.container{
    width: 100%;
}



.slider{
    width: 100%;
    height: 450rpx;
    position: relative;
    display: flex;
    flex-direction: column;
}
.slider .text{
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    padding: 6rpx 20rpx;
    background-color: rgba(0, 0, 0, 0.6);
    border-radius: 40rpx;
    font-size: 26rpx;
    color: #fff;
}
.slider swiper{
    width: 100%;
    height: 100%;
}
.slider swiper image{
    width: 100%;
    height: 100%;
    display: inline-block;
}


.content{
    display: flex;
    flex-direction: column;
    margin-bottom: 116rpx;
}
.content .title{
    width: 100%;
    padding:50rpx 42rpx;
    background: #fff;
    text-align: center;
    border-bottom: 1px solid rgba(0,0,0,0.1);
}
.content .title .name{
    margin-bottom:20rpx;
}
.content .title .name text:first-child{
    border: 2rpx solid #00a651;
    border-radius: 6rpx;
    padding: 0 10rpx 0 12rpx;
    color: #00a651;
    font-size: 26rpx;
    vertical-align: middle;
    margin-right: 18rpx;
}
.content .title .name text:last-child{
    font-size: 36rpx;color: #333;
    display: inline-block;
}
.content .title .addr{
    font-size: 24rpx;color: #666;
    letter-spacing: 1px;
}

.content .house{
    background: #fff;
    display: flex;
    flex-direction: column;
}
.pub-title{
    text-align: center;
    padding-top: 50rpx;
    padding-bottom: 60rpx;
    color: #333;
    font-size: 30rpx;
}
.pub-title::before,
.pub-title::after{
    content: 'ä¸€ ';
    color: #ccc;
}

.content .house .item{
    display: flex;
    flex-direction: row;
    padding: 0 40rpx;
    margin-bottom: 40rpx;
    font-size: 28rpx;
    color: #666;
}
.content .house .item > view{
    flex: 1;
    display: flex;
    justify-content: center;
}
.content .house .item > view:last-child{
    flex: 2.5;
}
.content .house .item > view > text{
    color:#ff801b;
    margin-right: 10rpx;
}

.content .house .h-more{
    background: #fff;
    text-align: center;
    width: 100%;
    margin-top: 20rpx;
    margin-bottom: 40rpx;
    display: flex;
    justify-content: center;
}
.content .house .h-more .mores{
    -webkit-border-radius: 6rpx;
    -moz-border-radius: 6rpx;
    border-radius: 6rpx;
    padding: 0 20rpx; 
    background: rgba(0, 0, 0, 0.04);
    height: 48rpx; 
    text-align: center;
    display: flex;
    align-items: center;
    
}
.content .house .h-more > view > view{
    display: inline-block;
    font-size: 24rpx;
    color: #bfbfbf;
}
.content .house .h-more > view > image{
    width: 24rpx;height: 14rpx;
    display: inline-block;
    margin-left: 10rpx;
}
.content .house .h-more .open{
    transform: rotate(180deg);
}

.content .house .dijia{
    padding: 40rpx 60rpx 40rpx 40rpx;
    background: #fff8f3;
    display: flex;
    align-items: center;
}
.content .house .dijia .left{
    width: 404rpx;
    font-size: 14px;
    color: #ff801b;
    margin-right: 70rpx;
}
.content .house .dijia .right{
    text-align: right;
    width: 178rpx;
}
.content .house .dijia .right > button{
    color: #fff;
    background: #ff801b;
    text-shadow: none;
    display: inline-block;
    height: 74rpx;
    width: 176rpx;
    -webkit-border-radius: 8rpx;
    -moz-border-radius: 8rpx;
    border-radius: 8rpx;
    font-size: 30rpx;
    border: 0;
    box-sizing: border-box;
    margin-top: 20rpx;
}
.content .house .new-msg{
    border-bottom: 1px solid rgba(0,0,0,0.1);
}
.content .house .new-msg .n-title {
    text-align: center;
    margin-bottom:50rpx;
}
.content .house .new-msg .n-title .pub-title {
    padding-bottom: 10rpx;
}
.content .house .new-msg .n-title .date{
    text-align: center;
    display: inline-block;
    width: 136rpx;
    height: 30rpx;
    line-height: 30rpx;
    background: rgba(125, 125, 125, 0.08);
    font-size: 24rpx;
    color: #999;
}
.content .house .new-msg .desc{
    padding:0 32rpx 76rpx 42rpx;
	font-size: 28rpx;
	line-height: 42rpx;
	letter-spacing: 1px;
	color: #666666;
}
.content .house .new-msg .btns{
    display: flex;
    justify-content: center;
    margin-bottom: 46rpx;
}
.content .house .new-msg .btns .btn{
    width: 316rpx;
	height: 80rpx;
	border-radius: 8rpx;
	border: 1px solid #ff801b;
    color: #ff801b;
    font-size: 32rpx;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
}

.content .house .room-type{
    display: flex;
    flex-direction: column;
    border-bottom: 1px solid rgba(0,0,0,0.1);
}
.content .house .room-type .scroll{
    display: flex;
    flex-direction: row;
    margin-bottom: 50rpx;
    margin-right: 40rpx;
}
.content .house .room-type .list{
    display: flex;
    flex-direction: row; 
    padding: 0 40rpx;
    width: 100%;
    box-sizing: border-box;
}
.content .house .room-type .list .r-item{
    display: flex;
    flex-direction: column;
    width: 420rpx;
    border: 1px solid rgba(0, 0, 0, 0.1);
    margin-right: 30rpx;
    box-shadow: 0 0 20rpx 2rpx rgba(0, 0, 0, 0.1);
}
.content .house .room-type .list .r-item .top{
    width: 100%;
	height: 350rpx;
    display: flex;
    border-bottom: solid 1px rgba(0, 0, 0, 0.1);
}
.content .house .room-type .list .r-item .top image{
    width: 420rpx;
    height: 100%;
}
.content .house .room-type .list .r-item .bottom{
    display: flex;
    flex-direction: column;
    padding: 30rpx 20rpx; 
    box-sizing: border-box;
}
.content .house .room-type .list .r-item .bottom .name{
    margin-bottom:20rpx;
}
.content .house .room-type .list .r-item .bottom .name text:first-child{
    border: 2rpx solid #00a651;
    border-radius: 6rpx;
    padding: 0 10rpx 0 12rpx;
    color: #00a651;
    font-size: 20rpx;
    vertical-align: middle;
    margin-right: 18rpx;
}
.content .house .room-type .list .r-item .bottom .name text:last-child{
    font-size: 28rpx;color: #333;
    display: inline-block;
}
.content .house .room-type .list .r-item .bottom .desc{
    display: flex;
    font-size: 28rpx;
    color: #ff801b;
}
.content .house .room-type .list .r-item .bottom .desc > view{
    margin-right:20rpx;
}

.content .house .romm-detail{
    display: block;
}
.content .house .romm-detail .d-list{
    display: flex;
    flex-direction: column;
    padding-left: 42rpx;
    font-size: 28rpx;
    color: #acacac;
}
.content .house .romm-detail .d-list .d-item{
    margin-bottom: 30rpx;
}
.content .house .romm-detail .d-list .value{
    color: #666666;
}

.content .space{
    width: 100%;
    height: 20rpx;
    background: rgba(0, 0, 0, 0.06);
}


.content .house .liangdian{
    display: block;
}
.content .house .liangdian .l-title{
    padding: 24rpx 38rpx;
    background: #fff;
    border-bottom: 1px solid rgba(0, 0, 0, 0.1);
    font-size: 32rpx;
    color: #333;
}
.content .house .liangdian .l-content{
    padding: 30rpx 45rpx 20rpx 36rpx;
    background: #fff;
}
.content .house .liangdian .l-content .l-tab{
    display: block;
    margin-bottom: 20rpx;
}
.content .house .liangdian .l-content .l-tab text{
    display: inline-block;
    padding: 18rpx 34rpx;
	border-radius: 10rpx;
    margin-bottom: 20rpx;
    margin-right: 28rpx;
    font-size: 28rpx;
    background: #fff;
    color: #898989;
    border: solid 1px #adadad;
}
.content .house .liangdian .l-content .l-tab text.active{
	border: solid 1px #ff801b;
    color: #ff801b;
}
.content .house .liangdian .l-content  .l-tabpane .pane{
    padding: 26rpx 56rpx 48rpx 24rpx;
    background: rgba(253,198,137,0.18);
    border-radius: 4px;
    color: #ff801b;
    font-size: 28rpx;
}

.content .house .youhui{
    padding: 20rpx;
    background: #f2f2f2;
}
.content .house .youhui .y-content{
    height: 188rpx;
    border-radius: 4px;
    position: relative;
    background: url(https://ssl.malmam.com/xiangju-static/dfs/16/3c8eba26124cdc/youhui_bg.png) no-repeat;
    background-size: cover;
    font-size: 32rpx;
    color: #fff;
}
.content .house .youhui .y-title{
    font-size: 24rpx;
    color: #ef5a3d;
    padding: 8rpx 0 0 20rpx;
}
.content .house .youhui .y-desc{
    padding: 30rpx 70rpx 32rpx 30rpx;
    font-size: 30rpx;
}

.content .house  .h-desc{
    display: block;
}
.content .house  .h-desc .h-content{
    padding: 20rpx;
    width: 100%;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #666;
}
.content .house  .h-desc .h-content p{
    display: block;
    width: 100%;
    font-size: 28rpx;
    margin-bottom: 20rpx;
    color: #666;
}
.content .house  .h-desc .h-content image{
    width: 100% !important;
    height: 500rpx;
}
.content .house  .h-desc .h-content .img{
    width: 100% !important;
    height: 500rpx;
}
.content .house  .h-desc .h-content .p1{
    margin-bottom: 30rpx;
}



.nav{
    position: fixed;
    left: 0;bottom: 0;
    width: 100%;
    display: flex;
    height: 116rpx;
    color: #fff;
    background: #f88b38;
    box-sizing: border-box;
    font-size: 32rpx;
}
.nav .n-left{
    width: 40%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #f88b38;
}
.nav .n-right{
    width: 60%;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    background: #ea6710;
}


.dialog{
    position: fixed;
    left: 0;top: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.6);
    z-index: 999999999;
}
.dialog .dialog-content{
    position: relative;
    width: 600rpx;
    height: auto;
    background: #fff;
    border-radius: 12rpx;
    left: 50%;top: 50%;
    transform: translate(-50%,-50%);
    color: #333;
    font-size: 32rpx;
    font-weight: 300;
}
.dialog .dialog-header{
    padding: 40rpx;
    text-align: center;
}
.dialog .dialog-header .title{
    color: #333;
    font-size: 36rpx;
    font-weight: 300;
}
.dialog .dialog-header .close{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 60rpx;height: 60rpx;
    position: absolute;
    top: 15px;
    right: 20px;
    font-size: 32px;
    font-weight: 100;
    color: #000;
    filter: alpha(opacity=20);
    opacity: 0.2;
}
.dialog .dialog-body{
    padding: 20rpx 50rpx;
    margin-bottom: 20rpx;
}
.dialog .good{
    text-align: left;
    padding-left: 50rpx;
}
.dialog .good .good1{
    font-size: 32rpx;
    font-weight: 400;
    display: block;
    margin-bottom: 14rpx;
}
.dialog .good .good2{
    font-size: 28rpx;
    color: red;
    display: block;
    margin-bottom: 14rpx;
}
.dialog .dialog-body .form-wrap{
    display: flex;
    margin-bottom: 34rpx;
    padding: 18rpx 24rpx;
    background-color: #f2f2f4;
    border-radius: 10rpx;
    text-align: left;
}
.dialog .dialog-body .form-wrap view{
    color: #333;
    width: 140rpx;
}
.dialog .dialog-body .form-wrap input{
    font-size: 28rpx;
    color: #333;
    filter: alpha(opacity=40);
    opacity: 0.4;
}

.dialog .dialog-footer{
    display: block;
}
.dialog .dialog-footer button{
    width: 100%;
    border-radius: 0px 0px 10rpx 10rpx;
    color: #fff;
    text-align: center;
    background-color: #FF801B;
}