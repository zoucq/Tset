.dialog{
    position: fixed;
    left: 0;top: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.6);
    z-index: 999999999;
}
.dialog .dialog-content{
    position: relative;
    width: 600rpx;
    height: auto;
    background: #fff;
    border-radius: 12rpx;
    left: 50%;top: 50%;
    transform: translate(-50%,-50%);
    color: #333;
    font-size: 32rpx;
    font-weight: 300;
}
.dialog .dialog-header{
    padding: 40rpx;
    text-align: center;
}
.dialog .dialog-header .title{
    color: #333;
    font-size: 36rpx;
    font-weight: 300;
}
.dialog .dialog-header .close{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 80rpx;height: 80rpx;
    position: absolute;
    top: 20rpx;
    right: 40rpx;
    font-size: 64rpx;
    font-weight: 100;
    color: #000;
    filter: alpha(opacity=20);
    opacity: 0.2;
}
.dialog .dialog-body{
    padding: 20rpx 50rpx;
    margin-bottom: 20rpx;
}
.dialog .good{
    text-align: left;
    padding-left: 50rpx;
}
.dialog .good .good1{
    font-size: 32rpx;
    font-weight: 400;
    display: block;
    margin-bottom: 14rpx;
}
.dialog .good .good2{
    font-size: 28rpx;
    color: red;
    display: block;
    margin-bottom: 14rpx;
}
.dialog .dialog-body .form-wrap{
    display: flex;
    align-items:flex-end;
    margin-bottom: 34rpx;
    padding: 18rpx 24rpx;
    /* background-color: #f2f2f4; */
    border-radius: 10rpx;
    text-align: left;
}
.dialog .dialog-body .form-wrap view{
    color: #333;
    /* width: 140rpx; */
    width: 110rpx;
    padding: 6rpx 0;
}
.dialog .dialog-body .form-wrap input{
    font-size: 28rpx;
    color: #333;
    /* filter: alpha(opacity=40);
    opacity: 0.4; */
    /* background-color:#f2f2f4; */
    padding:20rpx 12rpx 0 18rpx;
    border-bottom: 1px solid rgba(0,0,0,0.1);

}



.dialog .dialog-body .form-wrap.bar{
    padding: 10rpx 16rpx;
    font-size: 28rpx;
    background:rgba(255,246,244,1);
    border-radius:4rpx;
    border:1px solid rgba(255,197,197,1);
    box-sizing: border-box;
    /* margin:30rpx 24rpx; */
    margin:30rpx 32rpx 30rpx 24rpx;
}
.dialog .dialog-body .form-wrap.bar > view{
    width: auto;
    padding: 0;
    display: flex;
    align-items: center;
}
.dialog .dialog-body .form-wrap.bar,
.dialog .bar{
    display: flex;
    justify-content: space-between;
}
.dialog .dialog-body .form-wrap.bar  text,
.dialog .bar text{
    font-size: 28rpx;
    color:rgba(0,0,0,0.6);
}
.dialog .dialog-body .form-wrap.bar  image,
.dialog .bar image{
    display: inline-block;
    height: 14rpx;
    width: 24rpx;
    transform: rotate(-90deg);
}
.dialog .dialog-body .form-wrap.bar .color{
    color:rgba(237,75,75,1);
    font-weight: bold;
}
.dialog .dialog-body .form-wrap.bar  .b-more{
    color:rgba(0,0,0,0.36);
}

.dialog .dialog-footer{
    display: block;
}
.dialog .dialog-footer button{
    width: 100%;
    border-radius: 0px 0px 10rpx 10rpx;
    color: #fff;
    text-align: center;
    background-color: #FF801B;
}


.dialog-back .dialog-body{
    padding:0 58rpx;
}
.dialog-back .dialog-body .form-wrap{
