/* 加遮罩禁止滑动  */
.slider::before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: transparent;
    z-index: 1;
}
.slider swiper{
     /* width: 100%;height: 100%;   */

    position: relative;
    top: 0;
    left: 0;
    width:216rpx;
    height:68rpx; 

    box-sizing: border-box;
    overflow: hidden;
    
}
.slider image{
    display: inline-block;
    width: 100%;height: 100%;
}
.slider swiper swiper-item{
    display: flex;
    align-items: center;
    font-size: 20rpx;
    padding: 0 8rpx;
    color:rgba(255,67,53,1);

    position: absolute;
    top: 0;
    left: 0;
    width:216rpx;
    height:68rpx;
    background:rgba(255,240,201,1);
    box-shadow:0px 0px 12rpx 0px rgba(39,3,3,0.42);
    border-radius:0px 40rpx 40rpx 0px;
    border:1px solid rgba(255,163,126,1);
    border-left: 0;
    box-sizing: border-box;
}
.slider swiper swiper-item .avtor{
    display: inline-block;
    width:52rpx;
    height:52rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 8rpx;
}
.slider swiper swiper-item .avtor image{
    width: 100%;
    height: 100%;
}


.slider2{
    position: fixed;
    top: 20rpx;
    left: 0;
    width:216rpx;
    height:68rpx;
    /* background:rgba(255,240,201,1);
    box-shadow:0px 0px 12rpx 0px rgba(39,3,3,0.42);
    border:1px solid rgba(255,163,126,1); */
    border-radius:0px 40rpx 40rpx 0px;
    border-left: 0;
    box-sizing: border-box; 
    overflow: hidden;
    opacity: 0;
}


.slider2 image{
    display: inline-block;
    width: 100%;height: 100%;
}
.slider2 > view{
    display: flex;
    align-items: center;
    font-size: 20rpx;
    padding: 0 8rpx;
    color:rgba(255,67,53,1);

    width:216rpx;
    height:68rpx;
    background:rgba(255,240,201,1);
    box-shadow:0px 0px 12rpx 0px rgba(39,3,3,0.42);
    border-radius:0px 40rpx 40rpx 0px;
    border:1px solid rgba(255,163,126,1);
    border-left: 0;
    box-sizing: border-box;

    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}
.slider2  .avtor{
    display: inline-block;
    width:52rpx;
    height:52rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 8rpx;
}
.slider2 .avtor image{
    width: 100%;
    height: 100%;
}

.slider2.current{
    animation: show .6s ease , show2 .8s .6s ease ;
    -webkit-animation: show .6s ease , show2 .8s .6s ease ;
}

@keyframes show{
    from {
        left: -220rpx; top: 20rpx;opacity: 0;
    }
    to {
        left: 0; top: 20rpx;opacity: 1;
    }
}
@-webkit-keyframes show{
    from {
        left: -220rpx; top: 20rpx;opacity: 0;
    }
    to {
        left: 0; top: 20rpx;opacity: 1;
    }
}
@keyframes show2{
    from {
        top: 20rpx;opacity: 1;
    }
    to {
        top: -20rpx;opacity: 0;
    }
}
@-webkit-keyframes show2{
    from {
        top: 20rpx;opacity: 1;
    }
    to {
        top: -20rpx;opacity: 0;
    }
}

