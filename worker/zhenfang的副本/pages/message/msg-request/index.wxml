<view class='container'>

    <view class='boxs' wx:if='{{list.length>0}}'>
        <block wx:for="{{list}}" wx:key="lists">
            <view class='item'>
                <view class='top'>
                    <text>有一个问题需要您的指点</text>
                    <view>2018.01.06</view>
                </view>
                <view class='center'>
                    问题：这个楼盘怎么样？
                </view>
                <view class='down'>
                    <view>楼盘：西溪新城市</view>
                    <view>去回答</view>
                </view>
            </view>
        </block>
        <view wx:if="{{noMore}}" class='noMore'>无更多数据~~</view>
    </view>
    
    <view class='noList' wx:else>暂无数据~~</view>

</view>