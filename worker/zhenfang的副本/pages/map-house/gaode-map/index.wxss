.filter-content .addr .ad-boxs .left .item-addr:not(:first-child)
{
    padding-top: 40rpx;
} */



.filter-content .ad-btn{
    display: flex;
    justify-content:flex-end;
    width: 100%;
    padding: 28rpx 60rpx 28rpx 0;
    background: #f1f2f7;
}
.filter-content .ad-btn .btn{
    /* display: flex;
    justify-content: center;
    align-items: center; */
    width: 208rpx;height: 72rpx;
    border-radius: 100rpx;
    margin: 0;
    font-size: 32rpx;
    color: #3B55E7;
    border: 1px solid #3B55E7;
    display:inline-block;
    text-align:center;
    line-height:72rpx;
}


.filter-content .house-type{
    margin-top: 0;
}
.filter-content .house-type .ad-boxs .right view,
.filter-content .house-type .ad-boxs .right .radio
{
    border: 0;
    justify-content: flex-start;
    padding-left: 70rpx;
}
.filter-content .house-type .ad-boxs .right .radio-group label{
    justify-content: space-between;
    padding-left: 70rpx;
}

.filter-content .more{
    background:#fff;
}
.filter-content .more .ad-boxs{
    flex-direction: column;
    font-size: 30rpx;
    padding: 40rpx;
}
.filter-content .more .ad-boxs .list{
    flex-direction: row;
    display: flex;
    align-items: flex-start;
}
.filter-content .more .ad-boxs .list .title{
    margin: 0 10rpx;
    display: flex;
    justify-content: flex-start;
    flex: 0 0 150rpx;
    color: #3B3A4A;
    font-size: 28rpx;
}
.filter-content .more .ad-boxs .list .list-box{
    display: inline-block;
    margin-bottom: 20rpx;
}
.filter-content .more .ad-boxs .list:last-child .list-box{
    margin-bottom: -10rpx;
}
.filter-content .more .ad-boxs .list .list-box .item-more{
    display: inline-block;
    text-align: center;line-height: 80rpx;
    width: 160rpx;height: 80rpx;
    border-radius: 12rpx;
    background: #F4F5FF;
    color: #BBBCCD;
    font-size: 32rpx;
    margin-bottom: 20rpx;
}
.filter-content .more .ad-boxs .list .list-box .item-more:not(:nth-child(3n)){
    margin-right: 10rpx;
}
.filter-content .more .ad-boxs .list .list-box .item-more.active{
    border: 1px solid #A5ACFF;
    color: #5554FF;
}



.filter-content .more .ad-btn{
    padding:28rpx 0;
}
.filter-content .more .ad-btn .btn{
    margin-right: 40rpx;
    color: #828393;
    border: 1px solid #828393;
}
.filter-content .more .ad-btn .btn:last-child{
    color: #3B55E7;
    border: 1px solid #3B55E7;
}




.select-words{
    width: 100%; 
    height: auto;
    padding: 14rpx 0 0 24rpx; 
    white-space: normal;
    border-top: 1px solid #F4F3F3;
    /* position: fixed;
    top: 208rpx; */
}
.select-words .words-pd{
    padding: 14rpx 0 0 24rpx;
}
.select-words .text{
    display: inline-block;
    padding: 18rpx 38rpx;
    background: #F0F2FF;
    border-radius: 8rpx;
    font-size: 28rpx;
    color: #5554FF;
    margin-right: 20rpx;
    margin-bottom: 14rpx; 
    position: relative;
}
.select-words .img{
    display: inline-block;
    width: 30rpx;height: 30rpx;
    position: absolute;
    right: 0;top: 0;
}


 cover-view{
    overflow: visible;
    white-space: normal; 
} 




.see{
    width: 32rpx;height: 32rpx;
    display: inline-block;
}



.distance-test{
    position: fixed;
    right: 10rpx;
    bottom: 50rpx;
    width: 260rpx;height: 260rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
    z-index: 9999;

}


.distance-test .center{
    width: 60rpx;height: 60rpx;
    background: #0f0;
    color: #fff;
    /* display: flex;
    align-items: center;
    justify-content: center; */
    display: inline-block;
    text-align: center;
    line-height: 60rpx;
    position: absolute;
    z-index: 999;
}

.distance-test .left,
.distance-test .right,
.distance-test .cen{
    width: 60rpx;
    height: 60rpx;
    background: #f00;
    
    display: inline-block;
    text-align: center;
    line-height: 60rpx;
    border-radius: 50%;
    position: absolute;
    opacity: 0;
}
