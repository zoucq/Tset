page{
    background: #fff;
    width: 100%;height: 100%;
    overflow: hidden;
    border-top: 1px solid #E4E7F0;
}

cover-view{
    box-sizing: border-box;
}

.map_container{ 
    height: 100%; 
    width: 100%; 
} 

.map { 
    height: 100%; 
    width: 100%; 
} 

canvas{
    width: 100%;
    box-sizing: border-box;
    border: 1px solid red;
    z-index: 999;
}

.controls{
    background: #fff;
    position: fixed;
    bottom: 0; 
    /* position: relative;
    bottom: 0; */
    left: 0;
    display: flex;
    align-items: center;
    width: 100%;
    height: 200rpx;
}

.controls .left{
    padding: 0 30rpx;
}
.controls .right{
    font-size: 28rpx;
    display: flex;
    flex-direction: column;
    height: 160rpx;
    width: 100%;
    position: relative;
}
.controls .right .tel{
    position: absolute;
    right: 60rpx;
    top: 34rpx;
    display: flex;
    width: 88rpx;height: 88rpx;
    /* background: aquamarine;
    border-radius: 50%; */
}
.controls .right .tel cover-image{
    width: 100%;height: 100%;
}

.controls .right .name{
    margin-top: 8rpx;
    font-size: 32rpx;color: #3B3A4A;
}
.controls .right .price{
    margin-top: 20rpx;
    font-size: 28rpx;color: #F25C62;
}
.controls .right .biaoqian{
    margin-top: 20rpx;
    display: flex;
}
.controls .right .biaoqian .text{
    display: flex;
    justify-content: center;align-items: center;
    padding: 0 8rpx;
    font-size: 24rpx;
    color: #9E9DA3;
    border-radius: 8rpx;
    margin-right: 12rpx;
    background: #EFEEF8;
}
.controls .img{
    width: 200rpx;height: 160rpx;
}
.controls view{
    display: flex;
}






.container{
    display: flex;
    flex-direction: column;
    position: relative;
    background: #fff;
}
.container.active{
    display: flex;
    width: 100%;height: 100%;
    overflow: hidden;
}
.container .from{
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 28rpx 30rpx 28rpx 30rpx;  
    background: #fff;
    position: relative;  

}
/* .container .from .icon-sousuo{
    position: absolute; 
    left:58rpx; 
    display: flex;
    align-items: center;
    color: #b5b5c6;
} */
.container .from .txt{
    width: 100%;
    height:72rpx;
    border-radius: 32rpx;
    background: #F1F2F7;
    font-size: 28rpx;

    padding-left:22rpx;
    color:#BBBCCD;
    display:flex;
    align-items:center;
}
.container .from .btn{
    width: 20%;
    color: #3B55E7;
    font-size: 32rpx;
    background: #fff;
}
.container .from .btn::after{
    border: 0;
}

.container .filter{
    display: flex;
    padding: 24rpx 0; 
    font-size: 28rpx;
    overflow: hidden;
}
.container .filter .item{
    flex: 1;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    color: #BBBCCD;
}
.container .filter .item .icon-gengduo{
    font-size: 28rpx;
    display: flex;
    align-items: center;
    transform: rotate(90deg);
    margin-left: 8rpx; 
}
.container .filter .item.active{
    color: #3B3A4A;
}
.container .filter .item.active icon{
    color: #3B3A4A;
    transform: rotate(-90deg);
}

.container .filter .item .gengduo{
    width: 28rpx;height: 28rpx;
    font-size: 28rpx;
    display: flex;
    align-items: center;
    margin-left: 8rpx; 
}
.container .filter .item.active cover-view{
    color: #3B55E7;
}

.filter-content{
    display: flex;
    flex-direction: column;  
    width: 100%;
    height:100%;
}

.filter-content.active{
    background:rgba(0,0,0,0.4);
}
.filter-content > cover-view{
    background: #fff;
}
.filter-content .addr{
    border-top: 1px solid #E4E7F0;
    padding: 30rpx 0 0 0;
    background:#fff;
    font-size: 28rpx;
}
.filter-content .addr .ad-boxs{
    display: flex;
    color: #BBBCCD;
    height: 530rpx; 
    /* overflow: hidden;  */
}
.filter-content .addr .ad-boxs .left{
    flex: 0 0 312rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #fff;
}
.filter-content .addr .ad-boxs .right{
    flex:1;
    border-left: 6rpx solid #f1f3f7;
    margin-bottom: 40rpx;
    margin-right: 20rpx;
    height: 530rpx;
    overflow: scroll;
    background: #fff;
}
.filter-content .addr .ad-boxs .circle{
    display: inline-block;
    width:20rpx;
    height:20rpx; 
    background:#fff;
    border: 1px solid #BBBCCD;
    border-radius: 50%;
    margin-right: 32rpx; 
    text-align: left;
}

.filter-content .addr .ad-boxs .right .item-addr,
.filter-content .addr .ad-boxs .left .item-addr
{
    display: flex; 
    align-items: center; 
    font-size: 28rpx;
    height:72rpx;
}
.filter-content .addr .ad-boxs .right .item-addr{
    padding-left: 60rpx;
    height:72rpx;
}

.filter-content .addr .ad-boxs .left .active,
.filter-content .addr .ad-boxs .right .active{
    color: #3B3A4A;
    display: flex;
    align-items: center; 
}
.filter-content .addr .ad-boxs .left .active .circle,
.filter-content .addr .ad-boxs .right .active .circle
{
    background:#3B55E7;
    border: 1px solid #3B55E7; 
}


