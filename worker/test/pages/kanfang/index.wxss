
page{
    width: 100%;
    /* background:rgba(240,240,240,1); */
    border-top:1px solid rgba(192,192,192,1);
    background: #fff;
    height: 100vh;
    box-sizing: border-box;
}


.ul{
    display: flex;
    padding: 38rpx 0 46rpx;
    border-bottom: 1px solid rgba(192,192,192,1);
}
.ul .li{
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 28rpx;
    color: #858585;
}
.ul .li > view{
    margin-top: 8rpx;
}
.ul .li:nth-child(1) image{
    display: block;
    width: 110rpx;height: 48rpx;
}
.ul .li:nth-child(2) image{
    display: block;
    width: 54rpx;height: 60rpx;
}
.ul .li:nth-child(3) image{
    display: block;
    width: 47rpx;height: 60rpx;
}
.ul .li:nth-child(4) image{
    display: block;
    width: 78rpx;height: 68rpx;
}


.list{
    display: block;
    padding: 20rpx 20rpx 20rpx;
    height: 558rpx;
    overflow-x: hidden;
    overflow-y: auto; 
    background: rgba(240,240,240,1);
}
.list .item{
    display: flex;
    justify-content: space-between;
    align-items: center;
     padding: 0 20rpx; 
    /* padding: 62rpx 24rpx;   */
     height: 128rpx;  
    background: #fff;
    /* margin-bottom: 20rpx; */
    /* border-radius:8rpx; */
    border-bottom: 1px solid rgba(0,0,0,0.1);
    box-sizing: border-box;
    background: #FFFEF5;
}
.list .item:last-child{
    border-bottom: 0;
}
.list .item .i_left{
    display: flex;
    align-items: center;
    font-size: 32rpx;
    color:rgba(0,0,0,0.7);
    height: 100%;
}
.list .item .i_left .circle{
    display: inline-block;
    width:34rpx;
    height:34rpx;
    border-radius: 50%;
    border:1px solid rgba(0,0,0,0.28);
}
.list .item .i_left .circle-checked{
    display: inline-block;
    width:34rpx;
    height:34rpx;
    border-radius: 50%;
}
.list .item .i_left text{
    margin-left: 24rpx;
}

.list .item .i_right{
    display: flex;
    align-items: center;
    font-size: 24rpx;
    color:rgba(0,0,0,0.48);
    height: 100%;
}
.list .item .i_right image{
    display: inline-block;
    width:28rpx;
    height:16rpx;
    transform: rotate(-90deg);
    margin-left: 8rpx;
}


.form{
    display: flex;
    flex-direction: column;
    align-items: center;
    border-top: 1px solid rgba(192,192,192,1);
}
.form .wrap{
    display: flex;
    justify-content: center;
    font-size:32rpx;
    color: rgba(0,0,0,0.87);
    margin-top: 60rpx;
    height: 60rpx;
}
.form .wrap text{
    height:100%;
    display:flex;
    align-items:center;
    border-bottom: 1px solid rgba(0,0,0,0.1);
}
.form .wrap input{
    height:100%;
    display:flex;
    align-items:center;
    border-bottom: 1px solid rgba(0,0,0,0.1);
    color: #333;
}
.form .btn{
    width:620rpx;
    height:96rpx;
    text-align: center;
    line-height: 96rpx;
    background:rgba(209,46,51,1);
    border-radius:8rpx;
    font-size:36rpx;
    color:rgba(255,255,255,1);
    margin-top: 66rpx;
    /* margin-bottom: 80rpx; */
}




.dialog{
    position: fixed;
    left: 0;top: 0;
    width: 100%;
    height: 100vh;
    background: rgba(0,0,0,0.6);
    z-index: 999999999;
}
.dialog .dialog-content{
    position: relative;
    width: 600rpx;
    height: auto;
    background: #fff;
    border-radius: 12rpx;
    left: 50%;top: 50%;
    transform: translate(-50%,-50%);
    color: #333;
    font-size: 32rpx;
    font-weight: 300;
}
.dialog .dialog-header{
    padding: 40rpx;
    text-align: center;
}
.dialog .dialog-header .title{
    color: #333;
    font-size: 36rpx;
    font-weight: 300;
}
.dialog .dialog-header .close{
    display: flex;
    justify-content: center;
    align-items: center;
    width: 80rpx;height: 80rpx;
    position: absolute;
    top: 20rpx;
    right: 40rpx;
    font-size: 64rpx;
    font-weight: 100;
    color: #000;
    filter: alpha(opacity=20);
    opacity: 0.2;
}
.dialog .dialog-body{
    padding: 20rpx 50rpx;
    margin-bottom: 20rpx;
}
.dialog .good{
    text-align: left;
    padding-left: 50rpx;
}
.dialog .good .good1{
    font-size: 32rpx;
    font-weight: 400;
    display: block;
    margin-bottom: 14rpx;
}
.dialog .good .good2{
    font-size: 28rpx;
    color: red;
    display: block;
    margin-bottom: 14rpx;
}
.dialog .dialog-body .form-wrap{
    display: flex;
    align-items:flex-end;
    margin-bottom: 34rpx;
    padding: 18rpx 24rpx;
    /* background-color: #f2f2f4; */
    border-radius: 10rpx;
    text-align: left;
}
.dialog .dialog-body .form-wrap view{
    color: #333;
    /* width: 140rpx; */
    width: 110rpx;
    padding: 6rpx 0;
}
.dialog .dialog-body .form-wrap input{
    font-size: 28rpx;
    color: #333;
    /* filter: alpha(opacity=40);
    opacity: 0.4; */
    /* background-color:#f2f2f4; */
    padding:20rpx 12rpx 0 18rpx;
    border-bottom: 1px solid rgba(0,0,0,0.1);

}



.dialog .dialog-body .form-wrap.bar{
    padding: 10rpx 16rpx;
    font-size: 28rpx;
    background:rgba(255,246,244,1);
    border-radius:4rpx;
    border:1px solid rgba(255,197,197,1);
    box-sizing: border-box;
    /* margin:30rpx 24rpx; */
    margin:30rpx 32rpx 30rpx 24rpx;
}
.dialog .dialog-body .form-wrap.bar > view{
    width: auto;
    padding: 0;
    display: flex;
    align-items: center;
}
.dialog .dialog-body .form-wrap.bar,
.dialog .bar{
    display: flex;
    justify-content: space-between;
}
.dialog .dialog-body .form-wrap.bar  text,
.dialog .bar text{
    font-size: 28rpx;
    color:rgba(0,0,0,0.6);
}
.dialog .dialog-body .form-wrap.bar  image,
.dialog .bar image{
    display: inline-block;
    height: 14rpx;
    width: 24rpx;
    transform: rotate(-90deg);
}
.dialog .dialog-body .form-wrap.bar .color{
    color:rgba(237,75,75,1);
    font-weight: bold;
}
.dialog .dialog-body .form-wrap.bar  .b-more{
    color:rgba(0,0,0,0.36);
}

.dialog .dialog-footer{
    display: block;
}
.dialog .dialog-footer button{
    width: 100%;
    border-radius: 0px 0px 10rpx 10rpx;
    color: #fff;
    text-align: center;
    background-color: #FF801B;
}


.dialog-back .dialog-body{
    padding:0 58rpx;
}
.dialog-back .dialog-body .form-wrap{
    display: flex;
    justify-content: center;
    padding: 0;
    margin: 0;
}
.dialog-back .dialog-body .form-wrap image{
    width: 186rpx;
    height: 186rpx;
    margin: 50rpx 0;
}
.dialog-back .dialog-body .text{
    font-size: 40rpx;
    color: #000;
    font-weight: 500;
    margin-bottom: 30rpx;
}
.dialog-back .dialog-body .back{
    font-size: 28rpx;
    color: #888888;
    margin-bottom: 80rpx;
    text-align: center;
}
.dialog-back .dialog-footer{
    border-radius:0px 0px 10rpx 10rpx;
    border-top: 1px solid #E5E5E5;
}
.dialog-back .dialog-footer button{
    background: #fff;
    color: #ED7A41;
    font-size:36rpx;
}
