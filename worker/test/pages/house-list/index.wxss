.container{
    width: 100%;
    background:#E8E9F2;
    position: relative;
}

.banner{
    display: block;
    width: 100%;
    height: auto;
    margin-bottom:12rpx;
}
.banner > image{
    width: 100%;
    height: 100%;
    display: block;
}

.desc{
    padding: 28rpx 32rpx;
    font-size: 28rpx;
    color: #B78E5C;
    background: #fff;
    margin-bottom: 20rpx;
    line-height:52rpx;
    text-align:justify;
}

.list{
    display: block;

}
.list .item{
    display: flex;
    flex-direction: column;
    padding: 24rpx 28rpx 34rpx;
    box-sizing: border-box;
    font-size: 28rpx;
    background: #fff;
    margin-bottom: 8rpx;
}
.list .item .house{
    display: flex;
}
.list .item .house .left{
    width:280rpx;
    height:208rpx; 
    margin-right: 28rpx;
}
.list .item .house .left image{
    width: 100%;height: 100%;
    border-radius: 12rpx;
}

.list .item .house .right{
    display: flex;
    flex-direction: column;
}
.list .item .house .right .name{
    font-size:32rpx;
    color: rgba(0,0,0,0.8);
    margin-bottom: 8rpx;
    width: 410rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-weight: bold; 
    /* font-weight: 600; */
    /* opacity: 0.8; */
}
.list .item .house .right .addr{
    font-size:24rpx;
    color: rgba(0,0,0,0.5);
    margin-bottom: 20rpx;
    width: 410rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.list .item .house .right .price{
    font-size:28rpx;
    color: rgba(242,92,98,1);
    margin-bottom: 20rpx;
    /* font-weight: 600;  */
    font-weight: bold; 
    /* opacity: 0.8; */
}
.list .item .house .right .bq{
    font-size:24rpx;
    color: #25B162;
}
.list .item .house .right .bq > view{
    display: inline-block; 
    padding: 0 12rpx;
    border-radius: 6rpx ; 
    border: 1px solid #25B162;
    margin-right: 16rpx;
    margin-bottom: 8rpx;
}


.list .item .detail{
    padding: 16rpx;
    background: #F8F5F4;
    border-radius: 6rpx ;
    font-size: 28rpx;
    color: rgba(0,0,0,0.7);
    border: 1px solid #ECEBEB;
    /* margin-top: 28rpx; */
    margin-top: 12rpx;
}

.list .item .liangdian{
    margin-top: 36rpx;
}
.list .item .liangdian .tit{
    color: #AE8447;
    font-size: 32rpx;
    margin-bottom: 24rpx;
    font-weight: 600;
}
.list .item .liangdian .wrap{
    width: 100%;
    display: block;
    white-space: wrap;
    margin-bottom: 12rpx;
}
.list .item .liangdian .wrap .w-li{
    display: inline-block;
    /* padding: 12rpx 20rpx; */
    padding: 12rpx 16rpx;
    border-radius:16rpx;
    font-size:28rpx;
    color:rgba(223,149,35,1);
    background: #fff;
    border:1px solid rgba(238,174,75,1);
    /* margin-right: 28rpx; */
    margin-right: 24rpx; 
    margin-bottom: 24rpx;
}
.list .item .liangdian .wrap .w-li.active{
    background:rgba(238,174,75,1);
    color: #fff;
}
.list .item .liangdian .w-desc{
    background:rgba(250,245,240,1);
    border-radius:8rpx;
    padding: 16rpx 20rpx;
    font-size:28rpx;
    color:rgba(180,117,19,0.7);
    text-align:justify;
}



.back-index{
    position: fixed;
    top: 20rpx;
    right: 20rpx;
    display:inline-block;
    width: 116rpx;
    height: 116rpx;

}
.back-index image{
    display: inline-block;
    width: 100%;height: 100%;
}


button{
   background-color:transparent; 
}


#b-open{
    width: 750rpx;
    height: 548rpx;
    margin-left: -50rpx;
}

#b-close{
    top:-15rpx;
    left:596rpx;
}


.new-more{
    display:flex;
    justify-content:center;
    position:relative;
    bottom:-85rpx;
}
.new-more view{
    width:216rpx;
    height:58rpx;
    border-radius:12rpx;
    border:1px solid rgba(223,149,35,1);
    font-size:24rpx;
    color:rgba(202,124,0,1);
    text-align: center;
    line-height: 58rpx;
}


